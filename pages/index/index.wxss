page {
  height: 100vh;
}

.map_container {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}

.map {
  width: 100%;
  height: 100%;
}

.map_text {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0px;
  height: 160rpx;
  background: #fff;
  padding: 0 30rpx;
}

cover-image {
  width: 72rpx;
  height: 72rpx;
  margin: 10rpx auto;
  border-bottom: 4rpx solid #ddd;
}

cover-image:last-child {
  border-bottom: 0;
}

.controls {
  position: absolute;
  width: 80rpx;
  height: 175px;
  background: #fff;
  opacity: 0.9;
  right: 40rpx;
  bottom: 40rpx;
  border-radius: 10rpx;
}

.activeProject {
  width: 80%;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  background: #fff;
  font-size: 32rpx;
  position: absolute;
  top: 60rpx;
  left: 10%;
}

.location {
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  left: 40rpx;
  bottom: 40rpx;
  border-bottom: 0;
}

.desc {
  position: absolute;
  top: 40vh;
  text-align: center;
  width: 100%;
  font-weight: bold;
}

.mapButton {
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  background: #fff;
}

.getInfo {
  width: 200rpx;
  margin: 10rpx 0;
  line-height: 90rpx;
  position: relative;
  background: #fff;
  border-radius: 45rpx;
  padding-right: 30rpx;
  box-sizing: border-box;
  right: -50rpx;
}

.getRecord {
  width: 110rpx;
  line-height: 110rpx;
  border-radius: 50%;
  box-sizing: border-box;
  position: relative;
  z-index: 1000;
}

.getTrail {
  width: 200rpx;
  margin: 10rpx 0;
  line-height: 90rpx;
  position: relative;
  background: #fff;
  padding-left: 30rpx;
  box-sizing: border-box;
  border-radius: 45rpx;
  left: -50rpx;
}

.cu-modal {
  display: none;
  z-index: 10000001;
}

.cu-modal.show {
  overflow-y: hidden;
  display: block;
}

.login.cu-dialog {
  position: absolute;
  top: 400rpx;
  left: 35rpx;
}

.cu-btn {
  width: 100px;
}

.dom {
  position: absolute;
  top: 200rpx;
  width: 80rpx;
  height: 80rpx;
  padding-top: 15rpx;
  right: 20px;
  background: #fff;
  box-sizing: border-box;
}

.domImage{
  margin:5rpx auto;width:50rpx;height:50rpx;
}
